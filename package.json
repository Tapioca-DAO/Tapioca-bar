{
    "name": "tapioca-bar",
    "license": "MIT",
    "scripts": {
        "compile": "NODE_ENV=compile hardhat compile --quiet && npm run lint:sol",
        "lint:ts": "eslint . --fix",
        "lint:sol": "prettier --write \"./contracts/**/*.sol\" && solhint \"./contracts/**/*.sol\" --fix",
        "lint": "npm run lint:ts && npm run lint:sol",
        "clean": "rm -rf ./cache && rm -rf ./artifacts && rm -rf ./.openzeppelin",
        "coverage": "NODE_ENV=compile hardhat compile --quiet && hardhat coverage --network hardhat",
        "contracts_size": "npm run compile && NODE_ENV=hardhat hardhat size-contracts",
        "typechain": "npx hardhat typechain",
        "test": "npx hardhat test",
        "test:arb": "NODE_ENV=arbitrum hardhat test",
        "test:main": "NODE_ENV=mainnet hardhat test",
        "sdk-gen": "npx hardhat exportSDK",
        "@comment deploy": "Deploy scripts",
        "deploy": "npx hardhat deploy",
        "deploy:mainnet": "npx hardhat deploy --network mainnet",
        "deploy:testnet": "npx hardhat run scripts/exportStagingAddresses.ts --network rinkeby",
        "deploy:testEnv": "hardhat deploy --tags testEnv --network",
        "deploy:multisig": "hardhat deploy --tags MultisigMock --network",
        "deploy:multicall": "hardhat deploy --tags Multicall --network",
        "deploy:mocks": "hardhat deploy --tags Mocks --network",
        "deploy:usd0": "hardhat deploy --tags USD0 --network",
        "deploy:oracleMock": "hardhat deploy --tags OracleMock --network",
        "deploy:yieldBox": "hardhat deploy --tags YieldBox --network",
        "deploy:yieldBoxAssets": "hardhat deploy --tags YieldBoxAssets --network",
        "deploy:penrose": "hardhat deploy --tags Penrose --network",
        "deploy:masterContract": "hardhat deploy --tags MasterContract --network",
        "deploy:marketsHelper": "hardhat deploy --tags MarketsHelper --network",
        "deploy:multiSwapperV2": "hardhat deploy --tags MultiSwapperV2 --network",
        "deploy:stableToUsd0Bidder": "hardhat deploy --tags StableToUSD0Bidder --network",
        "deploy:singularityModules": "hardhat deploy --tags SingularityModules --network",
        "deploy:proxyDeployer": "hardhat deploy --tags ProxyDeployer --network",
        "deploy:marketsProxy": "hardhat deploy --tags MarketsProxy --network",
        "prepare": "husky install"
    },
    "devDependencies": {
        "@boringcrypto/boring-solidity": "https://github.com/boringcrypto/BoringSolidity",
        "@ethersproject/abi": "^5.7.0",
        "@ethersproject/providers": "^5.7.0",
        "@layerzerolabs/lz-evm-messagelib-v2": "2.3.6",
        "@layerzerolabs/lz-evm-oapp-v2": "^2.3.6",
        "@layerzerolabs/lz-evm-protocol-v2": "2.3.6",
        "@layerzerolabs/lz-evm-v1-0.7": "2.3.6",
        "@layerzerolabs/solidity-bytes-utils": "^0.8.0",
        "@nomicfoundation/hardhat-chai-matchers": "^1.0.3",
        "@nomicfoundation/hardhat-foundry": "^1.1.1",
        "@nomicfoundation/hardhat-network-helpers": "^1.0.4",
        "@nomicfoundation/hardhat-toolbox": "^2.0.2",
        "@nomiclabs/hardhat-ethers": "^2.1.1",
        "@nomiclabs/hardhat-etherscan": "^3.1.7",
        "@openzeppelin/contracts": "^4.9.5",
        "@swc/core": "^1.5.24",
        "@typechain/ethers-v5": "^10.1.0",
        "@typechain/hardhat": "^6.1.2",
        "@types/chai": "^4.2.22",
        "@types/inquirer": "^9.0.3",
        "@types/lodash": "^4.14.185",
        "@types/mocha": "^9.0.0",
        "@types/node": "^16.11.11",
        "@types/uuid": "^9.0.1",
        "@typescript-eslint/eslint-plugin": "^5.11.0",
        "@typescript-eslint/parser": "^5.11.0",
        "chai": "^4.3.6",
        "dotenv": "^10.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "eth-sig-util": "^3.0.0",
        "ethereumjs-utils": "^5.2.5",
        "ethereumjs-wallet": "^1.0.2",
        "ethers": "^5.7.0",
        "hardhat": "^2.13.0",
        "hardhat-contract-sizer": "^2.5.1",
        "hardhat-deploy": "^0.9.6",
        "hardhat-gas-reporter": "^1.0.8",
        "hardhat-tracer": "^2.2.2",
        "http-server": "^14.0.0",
        "mocha": "^9.1.3",
        "prb-math": "^2.4.1",
        "prettier": "^2.5.1",
        "prettier-plugin-solidity": "^1.1.3",
        "solc": "^0.8.4",
        "solhint": "^3.3.6",
        "solidity-coverage": "^0.8.1",
        "solidity-math": "^0.1.0",
        "ts-node": "^10.7.0",
        "typechain": "^8.1.0",
        "typescript": "^4.4.3",
        "write-json-file": "4.3.0"
    },
    "dependencies": {
        "@layerzerolabs/lz-v2-utilities": "2.3.6",
        "@nomiclabs/hardhat-vyper": "^3.0.1",
        "@openzeppelin/contracts-upgradeable": "^4.8.1",
        "@primitivefi/hardhat-dodoc": "^0.2.3",
        "@uniswap/v3-core": "^1.0.1",
        "@uniswap/v3-periphery": "^1.4.3",
        "husky": "^8.0.1",
        "inquirer": "^8.2.5",
        "load-json-file": "^7.0.1",
        "lodash": "^4.17.21",
        "tapioca-sdk": "^1.8.14",
        "tsconfig-paths": "^4.2.0",
        "uuid": "^9.0.0"
    }
}
